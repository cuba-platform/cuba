<screen-config xmlns="http://schemas.haulmont.com/cuba/4.1/screens.xsd">

    <include file="/com/haulmont/cuba/gui/screen-config.xml"/>

    <screen id="excelExport"
            template="/com/haulmont/cuba/web/app/ui/export/excel/excel-export.xml"/>

    <screen id="settings"
            template="/com/haulmont/cuba/web/app/ui/core/settings/settings-window.xml"/>

    <screen id="feedback"
            template="/com/haulmont/cuba/web/app/ui/core/feedback/feedback-window.xml"/>

    <screen id="logWindow"
            class="com.haulmont.cuba.web.log.LogWindowLauncher"/>

    <screen id="ftsSearch"
            class="com.haulmont.fts.web.ui.results.SearchLauncher"/>

    <screen id="ftsSearchResults"
            template="/com/haulmont/fts/web/ui/results/search-results.xml"/>

    <screen id="entityRestore"
            template="/com/haulmont/cuba/web/app/ui/core/restore/entity-restore.xml"/>

    <screen id="saveSetInFolder" template="/com/haulmont/cuba/web/app/folders/save-set-window.xml"/>

    <!-- JMX control -->
    <screen id="jmxConsole"
            template="/com/haulmont/cuba/web/app/ui/jmxcontrol/browse/display-mbeans.xml" multipleOpen="true"/>

    <screen id="jmxConsoleInspectMbean"
            template="/com/haulmont/cuba/web/app/ui/jmxcontrol/inspect/mbean-inspect.xml"/>

    <screen id="jmxConsoleEditAttribute"
            template="/com/haulmont/cuba/web/app/ui/jmxcontrol/inspect/attribute/attribute-edit.xml"/>

    <screen id="jmxConsoleOperationResult"
            template="/com/haulmont/cuba/web/app/ui/jmxcontrol/inspect/operation/operation-result.xml"/>

    <screen id="sys$JmxInstance.lookup"
            template="/com/haulmont/cuba/web/app/ui/jmxinstance/browse/jmxinstance-browse.xml"/>

    <screen id="sys$JmxInstance.edit"
            template="/com/haulmont/cuba/web/app/ui/jmxinstance/edit/jmxinstance-edit.xml"/>
    <!-- -->

    <screen id="sec$ScreenHistory.browse"
            template="/com/haulmont/cuba/web/app/ui/security/history/screen-history-browse.xml"/>

    <screen id="serverLog"
            template="/com/haulmont/cuba/web/app/ui/serverlogviewer/server-log-window.xml" multipleOpen="true"/>

    <screen id="serverLogAddLoggerDialog"
            template="/com/haulmont/cuba/web/app/ui/serverlogviewer/addition-logger-window.xml" multipleOpen="true"/>

    <screen id="serverLogLoggerControlDialog"
            template="/com/haulmont/cuba/web/app/ui/serverlogviewer/control-logger-window.xml" multipleOpen="true"/>

    <screen id="printDomain"
            class="com.haulmont.cuba.web.app.domain.DomainProvider"/>
</screen-config>
